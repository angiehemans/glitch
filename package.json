{
  "name": "glitch",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run db:generate && concurrently \"npm run dev:next\" \"npm run dev:prisma\" --names \"NEXT,PRISMA\" --prefix-colors \"cyan,magenta\"",
    "dev:next": "next dev --turbopack",
    "dev:prisma": "npx prisma dev",
    "dev:simple": "npm run db:generate && next dev --turbopack",
    "build": "npm run db:generate && npm run db:deploy && next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "setup": "node scripts/setup.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "@tabler/icons-react": "^3.35.0",
    "@vercel/blob": "^2.0.0",
    "bcryptjs": "^3.0.2",
    "next": "15.5.4",
    "next-auth": "^4.24.11",
    "prisma": "^6.16.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
